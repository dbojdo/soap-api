{
    "name" : "webit/soap-api",
    "type" : "library",
    "description": "Web-IT SOAP API Tools",
    "keywords": ["soap", "api", "soap wrapper", "soap sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel@bojdo.eu"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-soap": "*",
        "doctrine/collections": "^1.2",
        "doctrine/cache": "^1.2"
    },
    "require-dev": {
        "jms/serializer": "^2.0|^3.0",
        "phpunit/phpunit": "^9.6.5",
        "mockery/mockery": "^1.5.1",
        "behat/behat": "^3.0"
    },
    "suggest": {
        "jms/serializer": "To support normalization / hydration based on JMS Serializer. Minimum version: 1.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\SoapApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\SoapApi\\Tests\\": "tests/",
            "Webit\\SoapApi\\Features\\": "features/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-2.x": "2.x-dev",
            "dev-master": "3.x-dev"
        }
    }
}
